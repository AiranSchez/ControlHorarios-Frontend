  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/testSetupFile.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest"
    }
